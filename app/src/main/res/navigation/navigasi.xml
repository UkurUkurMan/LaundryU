<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigasi"
    app:startDestination="@id/orderFrag">

    <fragment
        android:id="@+id/orderComplete"
        android:name="com.laundryukurukur.OrderComplete"
        android:label="fragment_order_complete"
        tools:layout="@layout/fragment_order_complete" >
        <action
            android:id="@+id/action_orderComplete_to_orderNote"
            app:destination="@id/orderNote"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/orderNote"
        android:name="com.laundryukurukur.OrderNote"
        android:label="fragment_order_note"
        tools:layout="@layout/fragment_order_note" />
    <fragment
        android:id="@+id/orderFrag"
        android:name="com.laundryukurukur.OrderFrag"
        android:label="fragment_order"
        tools:layout="@layout/fragment_order" >
        <action
            android:id="@+id/action_orderFrag_to_orderComplete"
            app:destination="@id/orderComplete" />
    </fragment>
</navigation>